server {
        listen 80;
        listen [::]:80;

        root /home/patricia/www/hw01.dialnerd.me;

        index index.html;

        server_name hw01.dialnerd.me www.hw01.dialnerd.me;

        location / {
                # First attempt to serve request as file, then
                # as directory, then fall back to displaying a 404.
                try_files $uri $uri/ =404;
        }
}
